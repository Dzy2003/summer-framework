<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test_03.dao.IUserDao">

    <select id="queryUserInfoById" parameterType="test_03.po.User1" resultType="test_03.po.User1">
        SELECT *
        FROM users
        where uid = #{uid}
    </select>
    <select id="queryUsersInfoById" resultType="test_03.po.User1">
        SELECT *
        FROM users
        where uage = #{age}
    </select>

</mapper>
